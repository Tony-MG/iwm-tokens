      "$value": "{Mouse Grey.MouseGrey-800}"
    },
    "Col-Text-Checkbox": {
      "$type": "color",
      "$value": "{Sand.Sand-800}"
    },
    "Col-Stroke-Checkbox": {
      "$type": "color",
      "$value": "{Sand.Sand-800}"
    }
  },
  "Header": {
    "Col-text-header_navigation-default": {
      "$type": "color",
      "$value": "{Mouse Grey.MouseGrey-800}"
    },
    "Col-tab_header_bg-default": {
      "$type": "color",
      "$value": "{Backgrounds.Neutrals.Default}"
    },
    "Col-bg-header_mobile-app_icon": {
      "$type": "color",
      "$value": "{Mouse Grey.MouseGrey-700}"
    },
    "Col-bg-header_mobile_search": {
      "$type": "color",
      "$value": "{Sand.Sand-100}"
    },
    "Col-bg-Sub_nav_desktop": {
      "$type": "color",
      "$value": "{Colourless.XtraLightGrey}"
    }
  },
  "Tables": {
    "Col-bg-table_header": {
      "$type": "color",
      "$value": "{Backgrounds.Neutrals.Neutral_Separator}"
    },
    "Color-separator-table_header": {
      "$type": "color",
      "$value": "{Stroke.Default}"
    },
    "Col-bg-table-pair": {
      "$type": "color",
      "$value": "{Colourless.XtraLightGrey}"
    },
    "Col-bg-default-impair": {
      "$type": "color",
      "$value": "{Backgrounds.Neutrals.Default}"
    }
  },
  "Footer": {
    "Col-bg-Footer-Megamenu": {
      "$type": "color",
      "$value": "{Backgrounds.Neutrals.Neutral-800}"
    },
    "Col-bg-Footer-Primary": {
      "$type": "color",
      "$value": "{Backgrounds.Neutrals.Neutral-900}"
    }
  },
  "Rating": {
    "Col-bg-Rating-active": {
      "$type": "color",
      "$value": "{Backgrounds.Brand.Secondary}"
    }
  },
  "File Upload": {
    "Col-bg-File-Uploard-Default": {
      "$type": "color",
      "$value": "{Mouse Grey.MouseGrey-100}"
    }
  },
  "Dropdown": {
    "Col-bg-Dropdown-default": {
      "$type": "color",
      "$value": "{Sand.Sand-100}"
    },
    "Col-bg-Dropdown-hover": {
      "$type": "color",
      "$value": "{Sand.Sand-200}"
    },
    "Col-bg-Dropdown-hover 2": {
      "$type": "color",
      "$value": "{Sand.Sand-300}"
    }
  },
  "Charts": {
    "Col-bg-chart-4": {
      "$type": "color",
      "$value": "{Cherry}"
    },
    "Col-bg-chart-6": {
      "$type": "color",
      "$value": "{Orange}"
    },
    "Col-bg-chart-3": {
      "$type": "color",
      "$value": "{Pink}"
    },
    "Col-bg-chart-1": {
      "$type": "color",
      "$value": "{Light Blue}"
    },
    "Col-bg-chart-sand": {
      "$type": "color",
      "$value": "{Purple}"
    },
    "Col-bg-chart-7": {
      "$type": "color",
      "$value": "{Light Green}"
    },
    "Col-bg-chart-5": {
      "$type": "color",
      "$value": "{Deep Orange}"
    },
    "Col-bg-chart-8": {
      "$type": "color",
      "$value": "{Mouse Grey.MouseGrey-500}"
    }
  },
  "Buttons": {
    "horizontal_Padding": {
      "$type": "spacing",
      "$value": "32"
    },
    "vertical_Padding": {
      "$type": "spacing",
      "$value": "{2XSmall}"
    },
    "Primary": {
      "Default": {
        "background-color": {
          "$type": "color",
          "$value": "{Backgrounds.Brand.Primary}"
        },
        "content_color": {
          "$type": "color",
          "$value": "{Colourless.White}"
        }
      },
      "Hover": {
        "background-color": {
          "$type": "color",
          "$value": "{Backgrounds.Brand.Secondary}"
        },
        "content_color": {
          "$type": "color",
          "$value": "{Brand.Brand_Black}"
        }
      },
      "border-color": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0",
              "space": "srgb"
            }
          }
        },
        "$type": "color",
        "$value": "{Colourless.White}"
      },
      "Inactive": {
        "background-color": {
          "$type": "color",
          "$value": "{Backgrounds.Neutrals.Neutral_Inactive}"
        },
        "content_color": {
          "$type": "color",
          "$value": "{Texts.Inactive}"
        }
      },
      "Selected": {
        "background-color": {
          "$type": "color",
          "$value": "{Backgrounds.Colored.Colored-500}"
        },
        "content_color": {
          "$type": "color",
          "$value": "{Brand.Brand_Black}"
        }
      }
    },
    "Secondary": {
      "border-color": {
        "$type": "color",
        "$value": "{Brand.Brand_Primary}"
      },
      "content_color": {
        "$type": "color",
        "$value": "{Brand.Brand_Primary}"
      },
      "background-color": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0",
              "space": "srgb"
            }
          }
        },
        "$type": "color",
        "$value": "{Colourless.White}"
      }
    },
    "font-size": {
      "$type": "fontSizes",
      "$value": "{Medium}"
    },
    "label_font-family": {
      "$type": "fontFamilies",
      "$value": "{Urbanist}"
    },
    "elements-spacing": {
      "$type": "spacing",
      "$value": "{2XSmall}"
    },
    "border-radius": {
      "$type": "other",
      "$value": "4"
    }
  },
  "radius": {
    "$type": "borderRadius",
    "$value": "{Large}"
  },
  "Buttons-Label": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Buttons.label_font-family}",
      "fontSize": "{Buttons.font-size}"
    }
  }
}